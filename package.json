{
  "name": "mock-publicering",
  "displayName": "mock-publicering",
  "publisher": "satansdeer",
  "description": "",
  "version": "0.4.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mock-publicering.incrementStepIds",
        "title": "Increment step ids"
      },
      {
        "command": "mock-publicering.decrementStepIds",
        "title": "Decrement step ids"
      },
      {
        "title": "Enable CodeLens",
        "command": "codelens-sample.enableCodeLens",
        "category": "CodeLens Sample"
      },
      {
        "title": "Disable Codelens",
        "command": "codelens-sample.disableCodeLens",
        "category": "CodeLens Sample"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "mock-publicering.incrementStepIds",
          "group": "mockPublicering"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "mock-publicering.decrementStepIds",
          "group": "mockPublicering"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@fullstackio/cq": "^7.0.1-treesitter.38",
    "cq-treesitter-engine": "^7.0.1-treesitter.19"
  }
}
